@{
    var favicon = (ViewBag.Favicon != null) ? ViewBag.Favicon : Url.Content($"{CloudAppConfig.TenantManagementUrl.TrimEnd('/')}/logos?type=favicon"); ;
}
<link rel="icon" href="@favicon" onerror="this.onerror=null;this.src='@Url.Content("~/images/" + GlobalAppSettings.Branding + "/application/" + IconFileNames.Favicon)';" />
