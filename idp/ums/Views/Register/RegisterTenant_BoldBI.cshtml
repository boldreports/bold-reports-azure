@{
    int planId;
    var dnsRegEx = @"/^[a-zA-Z0-9][a-zA-Z0-9-]{4,}[a-zA-Z0-9]$/";
    var emailRegex = @"/^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,63}|[0-9]{1,3})(\]?)$/";
    var passwordRegex = ValidationHelpers.PasswordRegex;
    var isDomainValid = ViewBag.IsDomainValid != null ? ViewBag.IsDomainValid : true;
    var isUserExists = ViewBag.IsUserExists != null ? ViewBag.IsUserExists : false;
    var emailExist = ViewBag.IsAuthenticated && !string.IsNullOrWhiteSpace(ViewBag.TenantInfo.Email);
    var subdomainList = (List<SubdomainStatus>)ViewBag.SubdomainList;
    var publicPlanList = (List<SaaSPlanDetail>)ViewBag.PublicPlanList;
    var domainKey = ViewBag.TenantDomainKey;
    var passwordRules = "<ul class='text-body' id='password_policy_rules'><li id='p_policy_length'><span class = 'icon'></span><span class='content'>6 characters</span></li><li id='p_policy_uppercase'><span class = 'icon'></span><span class='content'>1 uppercase letter</span></li><li id='p_policy_lowercase'><span class = 'icon'></span><span class='content'>1 lowercase letter</span></li><li id='p_policy_number'><span class = 'icon'></span><span class='content'>1 number</span></li><li id='p_policy_specialcharacter'><span class = 'icon'></span><span class='content'>1 special character</span></li></ul>";
    Layout = null;
    var requestUrl = new Uri(Context.Request.GetDisplayUrl());
    var domain = Syncfusion.Server.Base.CloudAppConfig.ProductHomeUrl(TenantType.BoldBI);
    var isBoldBIBranding = GlobalAppSettings.Branding == @AppBranding.Bold.Identifier;
    var analyticsTrackingEnabled = Syncfusion.Server.Base.CloudAppConfig.EnableAnalyticsTracking;
    var hidePrivacypolicy = ViewBag.HidePrivacyPolicy != null ? ViewBag.HidePrivacyPolicy : false;
    var isAuthenticatedUser = User.Identity.IsAuthenticated;
    var isMobileDevice = DeviceDetection.IsMobile;
    var requestedPlan = int.TryParse(QueryHelpers.ParseQuery(requestUrl.Query)["plan"], out planId);
    var isFreePlanRequest = planId == (int)SaaSPlan.SingleUserPlan;
    var isEmbedPlan = false;
    var isCouponInValid = ViewBag.IsCouponInValid != null ? ViewBag.IsCouponInValid : false;
    var hasVsDevOffer = ViewBag.HasVsDevOffer != null ? ViewBag.HasVsDevOffer : false;
}
<!DOCTYPE html>
<html>
<head>
    <title>Try Dashboards | Register | @GlobalAppSettings.BrandingDisplayName</title>
    <link rel="icon" href="@GlobalAppSettings.CdnDomain/static/company/@GlobalAppSettings.Branding/logo/v1/@IconFileNames.Favicon" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Sign Up for @GlobalAppSettings.BrandingDisplayName Today. Free for @CloudAppConfig.TrailPeriod days." />
    <meta name="keywords" content="dashboard, business intelligence, data visualization, data analytics, mobile bi, html5 dashboards, dashboard web designer, dashboard viewer, dashboard server, user management, white labeled dashboards, user based dashboards, dashboard schedules, collaborative dashboards, localizable dashboards, dashboard data alerts, tabbed dashboards, dashboard mobile, data agent, rebranding, embedding dashboards, embed dashboard, embedded dashboards, bi dashboards, visualizations, data connections, data solutions, dashboard azure deployment, dashboard service, dashboard filters, dashboard commenting, dashboard exporting, dashboard custom widgets, data integration in dashboard, dashboard cloud, dashboard saas, pay as you go dashboards" />
    <meta name="is_boldbi_branding" content="@isBoldBIBranding.ToString()" />
    <meta name="google_analytics:send_to" content="@CloudAppConfig.GoogleAnalyticSentTo.ToString()" />
    <meta name="analytics_tracking:enabled" content="@analyticsTrackingEnabled.ToString()" />
    @await Html.PartialAsync("_OGPContent", new ViewDataDictionary(ViewData) { { "Product", TenantType.BoldBI } })
    @if (requestUrl.Query.Contains("?plan"))
    {
        var uri = new Uri(requestUrl.AbsoluteUri);
        <link rel="canonical" href="@uri.GetLeftPart(UriPartial.Path)" />
    }

    <link rel="stylesheet" href="~/bundles/css/tenant-register-boldbi.css" asp-append-version="true"/>
    <script src="~/bundles/js/tenant-register.js" asp-append-version="true"></script>

    <script>
        var registerTenantUrl = "@Url.Action("RegisterTenant", "Register")";
        var checkDnsAvailable = "@Url.Action("IsDnsAvailable", "Register")";
        var verifyConfirmationEmailUrl = "@Url.Action("VerifyConfirmationEmail", "Register")";
        var resendActivationUrl = "@Url.Action("resendactivationcode", "Register")";
        var checkVerificationCodeUrl = "@Url.Action("CheckVerificationCode", "Register")";
        var logOutUrl = "@Url.Action("logout", "accounts")";
        var logInUrl = "@Url.Action("login", "accounts")";
        var activationCode = "", tenantInfoId = "";
        var isEmailExist =  @Json.Serialize(emailExist);
        var isMobile = "@isMobileDevice.ToString().ToLower()";
        var isCloudRegistration =  @Json.Serialize(true);
        var isReportsCloudRegistration =  @Json.Serialize(false);
        var isEmbedPlan =  @Json.Serialize(isEmbedPlan);
        var setCookieConsentUrl = "@Url.Action("SetCookieConsent", "Register")";
        var isCouponInValid = "@isCouponInValid";
        var hasVsDevOffer = "@hasVsDevOffer";
        var isAuthenticatedUser = "@isAuthenticatedUser";
        var tenantTypeId =  @Json.Serialize((int)TenantType.BoldBI);
        var validateCorporateMailUrl = "@Url.Action("IsCorporateEmail", "Register")";

        $(document).ready(function (e) {
            if (hasVsDevOffer.toLowerCase() == "true") {
                $('#boldbi-register-body').addClass('register-tenant-page').css('cssText', 'padding: 0 !important');
            }

            $('#cookiesubs').on('click', function () {
                $(".cookie-notification").remove();
                SetCookie();
            });
        });

        function SetCookie() {
            $.ajax({
                type: "POST",
                url: window.setCookieConsentUrl,
            });
        }

        @if(GlobalAppSettings.Branding == AppBranding.Syncfusion.Identifier && Syncfusion.Server.Base.CloudAppConfig.EnableAnalyticsTracking)
        {
            <text>
                 var analyticKeyUA = "@Syncfusion.Server.Base.CloudAppConfig.GoogleAnalyticKeyUA";
                (function(i, s, o, g, r, a, m) {
                    window['GoogleAnalyticsObject'] = 'ga';
                    window['ga'] = window['ga'] || function() {
                        (window['ga'].q = window['ga'].q || []).push(arguments)
                       }, window['ga'].l = 1 * new Date();
                    a = document.createElement('script'),
                        m = document.getElementsByTagName('script')[0];
                    a.async = 1;
                    a.src = '//www.google-analytics.com/analytics.js';
                    m.parentNode.insertBefore(a, m)
                   })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
                ga('create', analyticKeyUA, 'auto', { allowLinker: true });
                ga('require', 'linker');
                ga('linker:autoLink', ['@(GlobalAppSettings.BrandingDomain)', 'boldid.net']);
                ga('send', 'pageview');
            </text>
        }

        @if (GlobalAppSettings.Branding == AppBranding.Bold.Identifier && Syncfusion.Server.Base.CloudAppConfig.EnableAnalyticsTracking) {
            <text>
            var analyticKeyUA = "@Syncfusion.Server.Base.CloudAppConfig.GoogleAnalyticKeyUA";
            var analyticKeyAW = "@Syncfusion.Server.Base.CloudAppConfig.GoogleAnalyticKeyAW";
            var google_tag_manager = document.createElement("script");
            google_tag_manager.src = "https://www.googletagmanager.com/gtag/js?id=" + analyticKeyUA;
            google_tag_manager.async = 1;
            var parent_node = document.head || document.body;
            parent_node.appendChild(google_tag_manager);

            window.dataLayer = window.dataLayer || [];
            function gtag() { dataLayer.push(arguments); }
            gtag('set', 'linker', { 'domains': ['boldid.net'] });
            gtag('js', new Date());
            gtag('config', analyticKeyUA);
            gtag('config', analyticKeyAW);
            </text>
        }
    </script>
    @await Html.PartialAsync("_HotjarTracking")
    <link href="https://fonts.googleapis.com/css?family=Bitter:400,700|Heebo:400,700&display=swap" rel="stylesheet">
</head>
<body ng-app="serverApp" ng-controller="tenantController" id="body">
    <div id="boldbi-register-body" class="register-tenant-parent bold-cloud-register-body">
        <div class="register-tenant-block">

            <div class="col-xs-12 link-wrapper link-top display-none">
                @if (!hasVsDevOffer)
                {
                    <a class="need-help pull-right" href="@CloudAppConfig.ContactUsUrl(TenantType.BoldBI)" target="_blank">Need help?</a>
                }
                @if (isAuthenticatedUser)
                {
                    <a class="pull-right logout-url" href="@Url.Action("logout", "accounts")?returnUrl=@requestUrl.AbsoluteUri">Sign out</a>
                }
            </div>

            <div class="info-container no-padding col-xs-12 col-sm-12 col-md-6 col-lg-6">

                @if (hasVsDevOffer)
                {
                    @await Html.PartialAsync("BoldBI_Cloud_VSDev/_Features")
                }
                else
                {
                    <div class="reg-info ready-set-part col-xs-12">
                        <div class="reg-header col-xs-12 no-padding">

                            <a href="@CloudAppConfig.SyncfusionCloudHomeUrl">
                                <img class="reg-image" alt="syncfusion" src="https://cdn.boldbi.com/static/company/boldbi/logo/v1/login_logo.svg">
                            </a>

                            <h1 class="reg-header-info ready-set-header">Ready, Set, Go!</h1>
                        </div>
                        <div class="header-info-meassages col-xs-12 no-padding">
                            <span class="reg-info-span col-xs-12 no-padding">
                                <img class="reg-info-tick col-xs-2 no-padding" src="~/styles/less/images/bullet.svg" style="margin: 0 auto;">
                                <span class="reg-info-1 col-xs-10 no-padding"><b>Create a name</b> for your dashboard application. This will be your site name.</span>
                            </span>
                            <span class="reg-info-span col-xs-12 no-padding">
                                <img class="reg-info-tick col-xs-2 no-padding" src="~/styles/less/images/bullet.svg" style="margin: 0 auto;">
                                <span class="reg-info-2 col-xs-10 no-padding"><b>Complete the form.</b> We'll be in touch to learn your business goals and set up a personalized demo.</span>
                            </span>
                            <span class="reg-info-message-tablet col-xs-12 no-padding">
                                <img class="reg-info-tick col-xs-2 no-padding" src="~/styles/less/images/bullet.svg" style="margin: 0 auto;">
                                <span class="reg-info-3 col-xs-10 no-padding">Start <b>creating your dashboards</b> now.</span>
                            </span>
                        </div>

                        <img class="dashboard-image @(isAuthenticatedUser ? "authenticated-dashboard-image" : string.Empty) display-none" src="https://cdn.boldbi.com/static/registration/bold-bi/v1/bold-bi-illustration.png">
                    </div>
                }
            </div>
            <div class="form-container no-padding col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <main class="register-tenant-child col-xs-12 offer-design @(hasVsDevOffer ? "vsdevspace-top" : string.Empty)">
                    <div class="table-container col-xs-12 no-padding">

                        <div class="col-xs-12 link-wrapper link-bottom display-none no-padding @(isAuthenticatedUser ? "authenticated-link-wrapper" : string.Empty)">
                            @if (!hasVsDevOffer)
                            {
                                <a class="need-help" href="https://www.boldbi.com/contact" target="_blank">Need help?</a>
                            }
                            @if (isAuthenticatedUser)
                            {
                                <a href="@Url.Action("logout", "accounts")?returnUrl=@requestUrl.AbsoluteUri" class="logout-url">Sign out</a>
                            }
                        </div>

                        <div id="tenant-registeristration" class="register-tenant-container @(isAuthenticatedUser ? "authenticated-register-tenant-container" : "register-cloud-tenant-container")">
                            <h1 class="boldbi-registration-main-heading @(isFreePlanRequest || hasVsDevOffer ? "free-plan-header" : string.Empty)">Get Started with @GlobalAppSettings.BrandingDisplayName</h1>
                            @if (!isFreePlanRequest && !hasVsDevOffer)
                            {
                                <h2 class="boldbi-registration-sub-heading">Free @CloudAppConfig.TrailPeriod-day trial. No credit card required.</h2>
                            }

                            @if (!User.Identity.IsAuthenticated)
                            {
                                <div class="signin-instead sigin-margin">Already have a Bold ID or Syncfusion account?<a href="@Url.Action("Login", "Accounts")?returnUrl=@WebUtility.UrlEncode(requestUrl.AbsoluteUri)" class="signin-instead-link login-url" tabindex="16">Sign in</a></div>
                            }

                            <div class="register-tenant-bg">
                                <div class="register-tenant-fields-div @(emailExist ? "signed" : string.Empty)">
                                    <form id="register-tenant-form" class="register-tenant-fields-container" name="tenantRegistrationForm" ng-init="init()" ng-submit="onRegisterFormSubmit($event, tenantInformation)" novalidate autocomplete="off">
                                        @Html.AntiForgeryToken()
                                        <div id="outer-container">
                                            <div class="form-inline input-alignment text-body name-block">
                                                <div class="form-group input-alignment text-body first-name-block">
                                                    <input type="text" tabindex="2" id="txt-firstname" name="firstname" class="form-control name-block-text" placeholder="First name" ng-model="tenantInformation.FirstName" ng-disabled="@emailExist" ng-init="tenantInformation.FirstName='@ViewBag.TenantInfo.FirstName'" ng-maxlength="@FormFields.FirstName" required autocomplete="given-name" autofocus />
                                                    <span class="validation-message ng-cloak top-padding" ng-show="(tenantRegistrationForm.firstname.$touched || tenantRegistrationForm.firstname.$dirty) && (tenantRegistrationForm.firstname.$error.required)">Please enter first name</span>
                                                    <span class="validation-message ng-cloak top-padding" ng-show="tenantRegistrationForm.firstname.$dirty && tenantRegistrationForm.firstname.$error.maxlength">Must be no more than @FormFields.FirstName characters.</span>
                                                </div>
                                                <div class="form-group input-alignment text-body last-name-block">
                                                    <input type="text" tabindex="3" id="txt-lastname" class="form-control name-block-text" placeholder="Last name" name="lastName" ng-model="tenantInformation.LastName" ng-disabled="@emailExist" ng-init="tenantInformation.LastName='@ViewBag.TenantInfo.LastName'" ng-maxlength="@FormFields.LastName" autocomplete="family-name" />
                                                    <span class="validation-message ng-cloak top-padding" ng-show="tenantRegistrationForm.lastName.$dirty && tenantRegistrationForm.lastName.$error.maxlength">Must be no more than @FormFields.LastName characters.</span>
                                                </div>
                                            </div>
                                            <div class="form-div form-group input-alignment text-body email-address @(emailExist ? "signed" : string.Empty)">
                                                <input type="text" tabindex="4" id="txt-emailid" class="form-control" placeholder="Business Email address" name="email" autocomplete="off" ng-model="tenantInformation.Email" ng-disabled="@emailExist" ng-init="tenantInformation.Email='@ViewBag.TenantInfo.Email'" ng-maxlength="@FormFields.EmailAddress" ng-pattern="@emailRegex" ng-change="onEmailChange()" corporatemail ng-model-options="{ debounce: 1000 }" required />
                                                <span id="email-validation" class="validation-message ng-cloak top-padding" ng-show="(tenantRegistrationForm.email.$touched || tenantRegistrationForm.email.$dirty) && (tenantRegistrationForm.email.$error.required)">Please enter email address</span>
                                                <span class="validation-message ng-cloak" ng-show="!tenantRegistrationForm.email.$error.maxlength && tenantRegistrationForm.email.$error.pattern ">Invalid email address</span>
                                                <span class="validation-message ng-cloak" ng-show="tenantRegistrationForm.email.$error.maxlength ">Email address is too long. Must be no more than @FormFields.EmailAddress characters.</span>
                                                <span class="validation-message display-none" id="invalid-domain">An account with this email already exists. Click <a href="@Url.Action("Login", "Accounts")?returnUrl=@WebUtility.UrlEncode(requestUrl.AbsoluteUri)" class="login-url"> here </a>to log in with your account.</span>
                                                <span class="validation-message display-none" id="invalid-email">Invalid email address</span>
                                                <span class="validation-message" ng-show="tenantRegistrationForm.email.$error.corporatemail && !tenantRegistrationForm.email.$error.pattern">@(isAuthenticatedUser ? "We require a business email account" : "Please enter a valid business email address")</span>
                                                <span class="validation-message display-none" ng-hide="hideBusinessEmailValidation" id="corporate-email">@(isAuthenticatedUser ? "We require a business email account" : "Please enter a valid business email address")</span>
                                            </div>
                                            @if (!emailExist)
                                            {
                                                <div id="password-field" class="form-div form-group input-alignment text-body">
                                                    <input type="text" class="display-none">
                                                    <input type="password" class="fake-password" name="fake_passwordfield_to_turnoff_autocomplete">
                                                    <input type="password" tabindex="5" id="txt-password" class="form-control" placeholder="Password" name="password" autocomplete="off" data-toggle="popover" data-title="Password must contain:" data-trigger="focus" data-html="true" data-content="@passwordRules" ng-model="tenantInformation.Password" ng-maxlength="@FormFields.Password" ng-pattern="@passwordRegex" required />
                                                    <span class="su su-eye display-none show-hide-password"></span>
                                                    <div class="error-container">
                                                        <span class="validation-message ng-cloak top-padding password-rules" ng-show="(tenantRegistrationForm.password.$touched || tenantRegistrationForm.password.$dirty) && (tenantRegistrationForm.password.$error.required)">Please enter password</span>
                                                        <span class="validation-message ng-cloak password-rules" ng-show="!tenantRegistrationForm.password.$error.maxlength && tenantRegistrationForm.password.$error.pattern">Invalid password</span>
                                                        <span class="validation-message ng-cloak password-rules" ng-show="tenantRegistrationForm.password.$error.maxlength">Password is too long. Must be no more than @FormFields.Password characters.</span>
                                                    </div>
                                                </div>
                                            }
                                            @if (string.IsNullOrWhiteSpace(ViewBag.TenantInfo.Company))
                                            {
                                                <div class="form-div form-group input-alignment text-body">
                                                    <input type="text" tabindex="6" id="txt-company" class="form-control" placeholder="Company name" name="Company" ng-model="tenantInformation.Company" ng-disabled="@(!string.IsNullOrWhiteSpace(ViewBag.TenantInfo.Company))" ng-init="tenantInformation.Company='@ViewBag.TenantInfo.Company'" ng-maxlength="@FormFields.Company" required autocomplete="family-name" />
                                                    <span class="validation-message ng-cloak top-padding" ng-show="(tenantRegistrationForm.Company.$touched || tenantRegistrationForm.Company.$dirty) && (tenantRegistrationForm.Company.$error.required)">Please enter company name</span>
                                                    <span class="validation-message ng-cloak top-padding" ng-show="tenantRegistrationForm.Company.$dirty && tenantRegistrationForm.Company.$error.maxlength">Company name is too long. Must be no more than @FormFields.Company characters.</span>
                                                </div>
                                            }
                                            @if (string.IsNullOrWhiteSpace(ViewBag.TenantInfo.Phone))
                                            {
                                                <div class="form-div form-group input-alignment text-body">
                                                    <input type="text" tabindex="7" id="txt-Phone" class="form-control" placeholder="Phone number" name="Phone" ng-model="tenantInformation.Phone" ng-disabled="@(!string.IsNullOrWhiteSpace(ViewBag.TenantInfo.Phone))" ng-init="tenantInformation.Phone='@ViewBag.TenantInfo.Phone'" ng-maxlength="@FormFields.Contact" autocomplete="family-name" />
                                                    <span class="validation-message ng-cloak top-padding" ng-show="tenantRegistrationForm.Phone.$dirty && tenantRegistrationForm.Phone.$error.maxlength">Contact Number is too long. Must be no more than @FormFields.Contact characters.</span>
                                                </div>
                                            }

                                            @if (!hasVsDevOffer)
                                            {
                                                <div class="form-div form-group input-alignment plan-list">

                                                    @if (publicPlanList.Count > 0)
                                                    {
                                                        <select class="form-control selectpicker plan-list-outline" data-size="5" id="selected-plan" tabindex="8" name="">
                                                            @foreach (var publicPlan in ViewBag.PublicPlanList)
                                                            {
                                                                if (planId == publicPlan.PlanId)
                                                                {
                                                                    <option selected="" data-subtext="@publicPlan.Description" value="@publicPlan.PlanId">@publicPlan.Name</option>
                                                                }
                                                                else
                                                                {
                                                                    <option data-subtext="@publicPlan.Description" value="@publicPlan.PlanId">@publicPlan.Name</option>
                                                                }
                                                            }
                                                        </select>
                                                    }

                                                    <span class="popover-detail-text" data-toggle="popover" data-container='body' data-content="<a target= '_blank' href='@CloudAppConfig.PricingPageUrl(TenantType.BoldBI)'>Click here</a> for more details" data-html="true" data-original-title="" title=""><span class="su-help"></span></span>
                                                </div>
                                            }

                                            <div id="dns-container" ng-init="tenantInformation.TenantTypeId='@ViewBag.TenantInfo.TenantTypeId'" ng-model="tenantInformation.TenantTypeId">
                                                <div class="form-div form-group input-alignment dns-list">
                                                    @if (subdomainList.Count > 0)
                                                    {
                                                        <select class="form-control selectpicker" data-live-search="@(subdomainList.Count > 10 ? "true" : "false")" data-size="10" id="selected-dns" name="tenantInfoId" ng-init="tenantInformation.TenantInfoId='@ViewBag.subdomainList[0].TenantInfoId'" ng-model="tenantInformation.TenantInfoId">
                                                            @foreach (var subdomain in ViewBag.subdomainList)
                                                            {
                                                                <option value="@subdomain.TenantInfoId">@subdomain.DNS</option>
                                                            }
                                                        </select>
                                                        <span class="validation-message" id="start-new-site"><a href="javascript:void(0)" ng-click="onStartNewSite()">Start a new site</a></span>
                                                    }
                                                </div>
                                                <div class="input-group dns-field @(subdomainList.Count > 0 ? "hidden" : string.Empty) text-body">
                                                    <input type="text" tabindex="9" id="txt-dns" name="dns" class="form-control" placeholder="Site name" ng-init="tenantInformation.DNS='@ViewBag.TenantInfo.DNS'" ng-model="tenantInformation.DNS" ng-pattern="@dnsRegEx" ng-maxlength="@FormFields.SiteName" tenant-type="@((int)TenantType.BoldBI)" uniquedns ng-model-options="{ debounce: 1000 }" required min="6" />
                                                    <span class="su su-password-tick"></span>
                                                    <span class="validation-message ng-cloak" id="spinner" ng-show="loading"></span>
                                                    <span class="input-group-addon">@domainKey</span>
                                                    <div class="popover-detail-text" data-toggle="popover" data-container='body' data-trigger="hover" data-content="This will be your site name. You can configure your custom domain later." data-original-title="" title=""><span class="su-help"></span></div>
                                                </div>
                                                <span class="validation-message ng-cloak" ng-show="tenantRegistrationForm.dns.$error.validdns && tenantRegistrationForm.dns.$dirty"></span>
                                                <span class="validation-message ng-cloak" ng-show="(tenantRegistrationForm.dns.$touched || tenantRegistrationForm.dns.$dirty) && (tenantRegistrationForm.dns.$error.required)">Please enter site name</span>
                                                <span class="validation-message ng-cloak" ng-show="tenantRegistrationForm.dns.$error.pattern && tenantRegistrationForm.dns.$dirty && !tenantRegistrationForm.dns.$error.maxlength">Must be at least 6 characters. Numbers and letters only.</span>
                                                <span class="validation-message ng-cloak" ng-show="tenantRegistrationForm.dns.$dirty && tenantRegistrationForm.dns.$error.maxlength">Site name is too long. Must be fewer than @(FormFields.SiteName + 1) characters.</span>
                                                <span class="validation-message reserved-site ng-cloak" ng-show="tenantRegistrationForm.dns.$error.uniquedns && tenantRegistrationForm.dns.$dirty">This site name has been already taken.</span>
                                                <span class="validation-message reserved-site ng-cloak" ng-show="tenantRegistrationForm.dns.$error.uniquednsnotactivated && tenantRegistrationForm.dns.$dirty">
                                                    This site has been already taken.
                                                    @if (!emailExist)
                                                    {
                                                        <span id="email-confirmation">Try another or if it's yours, click <a href="@Url.Action("Login", "Accounts")?returnUrl=@WebUtility.UrlEncode(requestUrl.AbsoluteUri)" class="login-url"> here </a>to login.</span>
                                                    }
                                                    else
                                                    {
                                                        <span>Try another.</span>
                                                    }
                                                </span>
                                            </div>
                                            <input type="hidden" id="ReferrerUrl" name="ReferrerUrl" ng-model="tenantInformation.ReferrerUrl" ng-init="tenantInformation.ReferrerUrl='@ViewBag.ReferrerUrl'">
                                            <input type="hidden" id="secondaryreferrerurl" name="SecondaryReferrerUrl" ng-model="tenantInformation.SecondaryReferrerUrl" ng-init="tenantInformation.SecondaryReferrerUrl='@ViewBag.SecondaryReferrerUrl'">
                                            <div class="agreement-wrapper form-div @(subdomainList.Count > 0 ? "hidden" : string.Empty)">
                                                <div class="agreement-div material-checkbox @(emailExist ? "signed" : string.Empty)">
                                                    @Html.CheckBox("agreement", false, new { @required = "required", ng_model = "tenantInformation.AggrementAcceptance", ng_click = "checkboxValidation()", tabindex = "10" })
                                                    <label for="agreement" class="@(subdomainList.Count > 0 ? "hidden" : string.Empty)"> I have read and agree to Syncfusion's <a href="@domain/privacy" target="_blank" tabindex="11">privacy policy</a>, <a href="@domain/cookie-policy" target="_blank" tabindex="12">cookie policy,</a> and Bold BI's <a href="@domain/terms-of-use" target="_blank" tabindex="13">terms of use</a>. <a href="#" tabindex="14" id="agreement-popover" style="padding-right: 5px;" data-toggle="popover" data-placement="right" data-container='body' data-trigger="focus" data-content="I understand I will receive transactional emails relating to my account, including invoices, and documentation." data-original-title="" title="">Read More.</a></label>
                                                </div>
                                                @if (!string.IsNullOrWhiteSpace(Context.Request.Query[CloudAppConfig.KcUserQueryParameter]))
                                                {
                                                    if (ViewBag.KcCouponStatus)
                                                    {
                                                        <div class="coupon-status-div alert alert-success">
                                                            <span class="coupon-status">Coupon applied successfully</span>
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="coupon-status-div alert alert-danger">
                                                            <span class="coupon-status">Invalid coupon</span>
                                                        </div>
                                                    }
                                                }
                                            </div>
                                        </div>
                                        <input type="submit" tabindex="15" id="register-tenant" class="proceed-button" ng-disabled="tenantRegistrationForm.$invalid" @*ng-click="onRegisterFormSubmit($event, tenantInformation)"*@ value="@(subdomainList.Count > 0 ? "Continue" : "Start Creating Dashboards")" />
                                    </form>
                                </div>
                            </div>

                            @if (User.Identity.IsAuthenticated)
                            {
                                <div class="signin-instead sigin-margin">
                                    <a href="@Url.Action("Logout", "Accounts")?returnUrl=@WebUtility.UrlEncode(requestUrl.AbsoluteUri)" class="signin-instead-link logout-url">Sign in with a different account</a>
                                </div>
                            }
                        </div>
                    </div>
                </main>
            </div>
        </div>
        <div id="account-activation-popup">
            <div id="account-activation-popup-parent">
                <div id="account-activation-popup-content">
                    @if (isCouponInValid)
                    {
                        @await Html.PartialAsync("BoldBI_Cloud_VSDev/_CouponErrorMessage")
                    }
                </div>
            </div>
        </div>
        @if (hasVsDevOffer)
        {
            @await Html.PartialAsync("BoldBI_Cloud_VSDev/_FAQ")
            @await Html.PartialAsync("BoldBI_Cloud_VSDev/_TermsAndConditions")
        }
        @await Html.PartialAsync("../Register/_ExistingAccountLoginPopUp")
    </div>
    @if (Context.Request.Cookies["cookie-consent"] == null)
    {
        @await Html.PartialAsync("_CookieNotification", new ViewDataDictionary(ViewData) { { "Domain", domain } })
    }
    <footer class="col-xs-12">
        @if (!hidePrivacypolicy)
        {
            <div class="privacy-policy-wrapper">
                <div class="mobile-align col-md-6 col-sm-12 col-xs-12">
                    <a class="privacy-policy" href="@domain/privacy" target="_blank">Privacy Policy</a>
                    <hr class="footer-separator">
                    <a class="terms-of-use" href="@domain/terms-of-use" target="_blank">Terms of Use</a>
                    <hr class="footer-separator">
                    <a class="cookie-policy" href="@domain/cookie-policy" target="_blank">Cookie Policy</a>
                </div>
                <div class="mobile-align col-md-6 col-sm-12 col-xs-12">
                    <a class="copyright" href="@domain/copyright" target="_blank">Copyright © 2001 - @DateTime.UtcNow.Year Syncfusion Inc. All Rights Reserved.</a>
                </div>
            </div>
        }
    </footer>

</body>
</html>