@{
    var gridName = "TenantsManagement";
    var stripeCustomerLink = CloudAppConfig.StripeCustomerLink;
    var isMobileDevice = DeviceDetection.IsMobile.ToString().ToLower();
    var emailUrl = "https://intranet.syncfusion.com/customer/email/";
    var intranetDomain = "https://intranet.syncfusion.com/";
    var canDelete = Context.Request.Cookies["tenant:delete"] != null && bool.Parse(Context.Request.Cookies["tenant:delete"].ToString());
}
<script>
    var activateTenantUrl = "@Url.Action("ActivateTenant", gridName)";
    var suspendTenantUrl = "@Url.Action("SuspendTenant", gridName)";
    var deleteTenantUrl = "@Url.Action("DeleteTenant", gridName)";
    var markorremoveInternalUrl = "@Url.Action("MarkOrRemoveInternal",gridName)";
    var i_am_ie9 = false;
</script>
<!--[if LT IE  10]>
        <script>
            i_am_ie9 = true;
        </script>
    <![endif]-->
<div id="items" class="item-grid">
</div>
<script type="text/x-jsrender" id="owner-header">
    <div class="item-name">Owner</div>
</script>
<script type="text/x-jsrender" id="dns-header">
    <div class="item-header">DNS</div>
</script>
<script type="text/x-jsrender" id="stripe-customer-id-header">
    <div class="item-header">Stripe Customer ID</div>
</script>
<script type="text/x-jsrender" id="created-date-header">
    <div class="item-header">Created Date</div>
</script>
<script type="text/x-jsrender" id="app-status-header">
    <div class="item-header">App Status</div>
</script>
<script type="text/x-jsrender" id="actions-header">
    <div class="item-header">Actions</div>
</script>

<script type="text/x-jsrender" id="owner">
    <div class="item-description grid-content">
        @if (!bool.Parse(isMobileDevice))
        {
            <span class="customer-name" data-toggle="tooltip" title="{{:FullName}}" data-placement="bottom">
                <span>{{:FullName}}</span>

            </span>
            <span class="customer-email" data-toggle="tooltip" title="{{:EmailAddress}}" data-placement="bottom">
                <a rel="nofollow" href="@emailUrl{{:EmailAddress}}" target="_blank">
                    {{:EmailAddress}}
                </a>
            </span>
        }
        else
        {
            <div class="mobile-info">
                <h5 class="info-row info-name-status">
                    <span class="float-left info-name">{{:FullName}}</span>
                    <span class="float-right info-status">
                        {{include tmpl="#appStatus" /}}
                    </span>
                </h5>
                <p class="info-row info-email">{{:EmailAddress}}</p>
                <a class="info-row info-dns">{{:DNS}}</a>
            </div>
        }

    </div>
</script>

<script type="text/x-jsrender" id="internal-column">
    {{if IsInternal}}
    <img class="internal-icon" src="@Url.Content("~/content/images/" + GlobalAppSettings.Branding + "/syncfusion.ico")" />
    {{/if}}
</script>

<script type="text/x-jsrender" id="dns">
    <span><a href="{{:~urlEncode(Id, DNS)}}" target="_blank">{{:DNS}}</a></span>
</script>
<script type="text/x-jsrender" id="product-type">
    <div class="product-types">
        {{if TenantTypeId == "@((int)TenantType.BoldBI)"}}
        <span class="status-button bold-bi">@GlobalAppSettings.GetDescription(TenantType.BoldBI)</span>
        {{else TenantTypeId == "@((int)TenantType.BoldReports)"}}
        <span class="status-button bold-reports">@GlobalAppSettings.GetDescription(TenantType.BoldReports)</span>
        {{else TenantTypeId == "@((int)TenantType.BoldBIOnPremise)"}}
        <span class="status-button bold-bi-onpremise">@GlobalAppSettings.GetDescription(TenantType.BoldBIOnPremise)</span>
        {{else TenantTypeId == "@((int)TenantType.BoldReportsOnPremise)"}}
        <span class="status-button bold-reports-onpremise">@GlobalAppSettings.GetDescription(TenantType.BoldReportsOnPremise)</span>
        {{else TenantTypeId == "@((int)TenantType.BoldSign)"}}
        <span class="status-button bold-sign">@GlobalAppSettings.GetDescription(TenantType.BoldSign)</span>
        {{/if}}
    </div>
</script>
<script type="text/x-jsrender" id="stripe-customer-id">
    <div class="stripe-customer">
