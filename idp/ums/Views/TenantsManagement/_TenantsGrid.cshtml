            allowPaging: true,
            allowSorting: true,
            enableAltRow: false,
            allowSearching: true,
            allowFiltering: true,
            allowSelection: true,
            filterSettings: filters,
            actionBegin: "fnActionBegin",
            actionComplete: "fnActionComplete",
            enableRowHover: true,
            columns: [
                {
                    headerText: "",
                    template: true,
                    templateID: "#internal-column",
                    width: 30,
                    allowSorting: true,
                    allowFiltering: false,
                    visible: ("@isMobileDevice" == "false" && (window.innerWidth > 1040))
                },
                {
                    headerText: "Owner",
                    template: true,
                    headerTemplateID: "#owner-header",
                    templateID: "#owner",
                    width: ("@isMobileDevice" == "false" && (window.innerWidth > 1200)) ? ((window.innerWidth < 1400) ? 250 : 350) : 140,
                    field: "[@GlobalAppSettings.DBColumns.DB_User.DB_TableName].[@GlobalAppSettings.DBColumns.DB_User.DisplayName]",
                    type: "string",
                    allowFiltering: false
                },
                {
                    headerText: "DNS",
