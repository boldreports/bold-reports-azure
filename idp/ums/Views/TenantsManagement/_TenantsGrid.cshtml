            ],
            dataBound: function (args) {
                $("[data-toggle='tooltip']").tooltip();
            }
        });

        $.views.helpers({
            urlEncode: function (id, dns) {
                return encodeURI("/tenants?id=" + id + "&dns=" + dns + "&tab=general");
            }
        });
    });
</script>
