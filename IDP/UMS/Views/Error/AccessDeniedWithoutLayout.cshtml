@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>[[[Access Denied - ]]]@GlobalAppSettings.SystemSettings.OrganizationName</title>
    @if (!CloudAppConfig.IsSelfHosted)
    {
        <link rel="icon" href="@GlobalAppSettings.CdnDomain/static/company/@GlobalAppSettings.Branding/logo/v1/@IconFileNames.Favicon" />
    }
    else
    {
        <link rel="icon" href="@GlobalAppSettings.SystemSettings.FavIcon" onerror="this.onerror=null;this.src='@GlobalAppSettings.CdnDomain/@GlobalAppSettings.Branding/Application/@IconFileNames.Favicon';" />
    }
    @Styles.Render("~/styles/layout")
    @Styles.Render("~/styles/error-page")
</head>
<body>
    <div class="base-container-bg">
        <div class="base-conatiner" id="base-conatiner">
            <div id="main-div">
                <div class="app-logo">
                    @if (!CloudAppConfig.IsSelfHosted)
                    {
                        <img alt="[[[Login Logo]]]" src="@GlobalAppSettings.CdnDomain/static/company/@GlobalAppSettings.Branding/logo/v1/@IconFileNames.LoginLogo" />
                    }
                    else
                    {
                        <img alt="[[[Login Logo]]]" src="@GlobalAppSettings.SystemSettings.LoginLogo" onerror="this.onerror=null;this.src='@Url.Content("~/Content/images/" + GlobalAppSettings.Branding + "/application/" + IconFileNames.LoginLogo)';" />
                    }
                </div>
                <div class="secondary-div">
                    <div class="page-title">
                        [[[Access Denied]]]
                    </div>
                    <div class="page-content">[[[You do not have permission to view this page.]]]</div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>