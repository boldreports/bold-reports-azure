@{
    ViewBag.Title = "[[[Access Denied]]]";
    Layout = "~/Views/Shared/WolfPackMaster.cshtml";
    var globalAppSettings = _globalAppSettings;
    var reportServerResourceUrl = globalAppSettings.SystemSettings.CDNLink.TrimEnd('/');
}

@section scripts{
    <environment include="Development,CloudDevelopment">
        <script asp-append-version="true" src="@reportServerResourceUrl/cdn/scripts/bootstrap.js"></script>
    </environment>
    <environment exclude="Development,CloudDevelopment">
        <script asp-append-version="true" src="@reportServerResourceUrl/cdn/scripts/bootstrap.min.js"></script>
    </environment>
}

@section MainSection{
    <h2>[[[Access Denied]]]</h2>
}