@using Syncfusion.Server.Base.Helpers

@{
    var globalAppSettings = _globalAppSettings;
    ViewBag.Title = globalAppSettings.SystemSettings.OrganizationName + " - [[[Report Not Found]]]";
    ViewBag.MessageBox = false;
    var reportServerResourceUrl = globalAppSettings.SystemSettings.CDNLink.TrimEnd('/');
}
@if (Layout == null)
{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section scripts{
    <environment include="Development,CloudDevelopment">
        <script nonce="@Context.Items["ScriptNonce"]" asp-append-version="true" src="@reportServerResourceUrl/cdn/scripts/bootstrapjs.js" integrity="@IntegrityTagHelper.GetIntegrity("wwwroot/cdn/scripts/bootstrapjs.js")" crossorigin="anonymous"></script>
    </environment>
    <environment exclude="Development,CloudDevelopment">
        <script nonce="@Context.Items["ScriptNonce"]" asp-append-version="true" src="@reportServerResourceUrl/cdn/scripts/bootstrapjs.min.js" integrity="@IntegrityTagHelper.GetIntegrity("wwwroot/cdn/scripts/bootstrapjs.min.js")" crossorigin="anonymous"></script>
    </environment>
}
@section MainSection{
    <div>
        <div class="col-md-12">
            <div class="col-xs-12 text-center response-header">[[[Sorry, we could not find your report.]]]</div>
            <div class="response">[[[Possible reason:]]]<br>[[[The report could have been deleted.]]]</div>
        </div>
    </div>
}
