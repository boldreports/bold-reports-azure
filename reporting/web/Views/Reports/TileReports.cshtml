                                                <ul class="dropdown-menu dashboard-options" role="menu">
                                                    <li ng-if="dashboardItem.CanWrite && !isTablet">
                                                        <span class="web-designer option-click" title="" ng-click="editDashboard()">
                                                            <i class="su su-edit"></i>
                                                            <span>[[[Edit]]]</span>
                                                        </span>
                                                    </li>
                                                    <li ng-if="dashboardItem.CreatedById == currentUserId || isAdmin">
                                                        <span ng-if="!dashboardItem.IsPublic" title="" class="make-public option-click" ng-click="makePublic()">
                                                            <i class="su su-public"></i>
                                                            <span>[[[Make Public]]]</span>
                                                        </span>
