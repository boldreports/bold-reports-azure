<div id="delete-popup-container" class="col-lg-12 no-padding">
    <div class="col-md-12 header-menu header-pad-top">
        <div class="col-xs-6 no-padding">
            <span class="su su-delete head-icon"></span>
            <span class="popup-title">[[[Delete Schedule]]]</span>
        </div>
        <div class="col-xs-6 no-padding">
            <a href="#" title="[[[Close]]]" class="popup-close prevent-default-action"><span class="su su-close pull-right"></span></a>
        </div>
    </div>
    <div class="dialog-body col-xs-12 text-center no-padding">
        <div class="col-xs-12">
            <div class="delete-schedule-content">[[[Are you sure you want to delete this schedule]]] - <span class="highlight-name">@ViewBag.schedulename</span> ?</div>
        </div>
    </div>
    <div class="col-xs-12 dialog-footer text-right">
        <div class="col-xs-12 no-padding text-right validation-area">
            <input type="button" class="critical-action-button app-btn-danger text-right" value="[[[Yes]]]" id="delete-item" tabindex="1" autofocus />
            <input title="" type="button" id="Cancel" class="secondary-button app-btn-secondary left-margin text-right popup-close delete-schedule-dialog-close" value="[[[No]]]" tabindex="3" autofocus />
        </div>
        <div class="col-xs-12 no-padding text-center success-area" style="display:none">
            <input title="" type="button" id="Cancel" class="secondary-button app-btn-secondary left-margin text-right popup-close" value="[[[OK]]]" tabindex="3" autofocus />
        </div>
    </div>
</div>
